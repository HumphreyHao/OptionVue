,EAGP,YAHO,EAIP,eAJO,EAKL,MALK,CAvBJ;AA8BLC,eAAS,EAAE,IA9BN;AA+BLC,mBAAa,EAAE;AA/BV,KAAP;AAiCD,GApCY;AAqCbC,SAAO,EAAE;AACPC,eADO,uBACKC,KADL,EACY;AAAA;;AACjB,UAAI,KAAKJ,SAAL,KAAmBI,KAAvB,EAA8B;AAC5B,gBAAQ,KAAKH,aAAb;AACE,eAAK,IAAL;AACE,iBAAKA,aAAL,GAAqB,KAArB;AACA;;AACF,eAAK,KAAL;AACE,iBAAKA,aAAL,GAAqB,MAArB;AACA;;AACF,eAAK,MAAL;AACE,iBAAKA,aAAL,GAAqB,IAArB;AACA;AATJ;AAWF,OAZA,MAYO;AACL,aAAKA,aAAL,GAAqB,KAArB;AACF;;AAEA,WAAKD,SAAL,GAAiBI,KAAjB;;AAEA,UAAI,CAAC,KAAKH,aAAV,EAAyB;AACvB,aAAKb,IAAL,GAAYD,aAAa,CAAC,KAAKW,OAAN,EAAe,KAAKT,IAApB,CAAzB;AACA;AACF;;AAEA,WAAKD,IAAL,GAAY,KAAKA,IAAL,CAAUiB,IAAV,CACR,UAACC,IAAD,EAAOC,IAAP,EAAgB;AACd,YAAI,KAAI,CAACN,aAAL,KAAuB,MAA3B,EAAmC;AACjC,iBAAOM,IAAI,CAACH,KAAD,CAAJ,CAAYI,aAAZ,CAA0BF,IAAI,CAACF,KAAD,CAA9B,CAAP;AACF;;AAEA,eAAOE,IAAI,CAACF,KAAD,CAAJ,CAAYI,aAAZ,CAA0BD,IAAI,CAACH,KAAD,CAA9B,CAAP;AACF,OAPQ,CAAZ;AASD,KAlCM;AAmCPK,YAnCO,oBAmCEC,CAnCF,EAmCK;AACV,WAAKrB,IAAL,GAAYqB,CAAC,CAACC,MAAF,CAASC,KAArB;AACA,WAAKxB,IAAL,GAAYD,aAAa,CAAC,KAAKW,OAAN,EAAe,KAAKT,IAApB,CAAzB;AACF;AAtCO,GArCI;AA6EbwB,SA7Ea,qBA6EH;AACR,SAAKzB,IAAL,sBAAgB,KAAKU,OAArB;AACF;AA/Ea,CAAf","names":["performSearch","rows","term","results","filter","row","join","toLowerCase","includes","name","data","rawRows","columns","sortIndex","sortDirection","methods","sortRecords","index","sort","rowA","rowB","localeCompare","onSearch","e","target","value","mounted"],"sourceRoot":"","sources":["/Users/zhaohao/Downloads/DotNetRepos/OptionVue/src/components/Table.vue"],"sourcesContent":["<template>\n  <div class=\"p-10\">\n    <div>\n      <input\n          type=\"text\"\n          class=\"border-2 mb-5 rounded h-10 p-2\"\n          placeholder=\"Search records\"\n          @input=\"onSearch\"\n      />\n    </div>\n    <table>\n      <thead>\n      <tr>\n        <th\n            v-for=\"(column, index) in columns\"\n            v-bind:key=\"index\"\n            class=\"border-2 p-2 text-left\"\n            v-on:click=\"sortRecords(index)\"\n        >\n          {{ column }}\n        </th>\n      </tr>\n      </thead>\n      <tbody>\n      <tr\n          v-for=\"(row, index) in rows\"\n          v-bind:key=\"index\"\n      >\n        <td\n            v-for=\"(rowItem, itemIndex) in row\"\n            v-bind:key=\"itemIndex\"\n            class=\"border-2 p-2\"\n        >\n          {{ rowItem }}\n        </td>\n      </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<script>\n\nconst performSearch = (rows, term) => {\n  const results = rows.filter(\n      row => row.join(\" \").toLowerCase().includes(term.toLowerCase())\n  )\n\n  return results;\n}\n\nexport default {\n  name: 'Table',\n  data() {\n    return {\n      term: '',\n      rawRows: [\n        [\n          \"Manoj\", \"24\", \"Software Developer\", \"1997\", 0.2\n        ],\n        [\n          \"John\", \"26\", \"Lawyer\", \"1995\", 0.3\n        ],\n        [\n          \"Lily\", \"34\", \"Saleswoman\", \"1987\", 0\n        ],\n        [\n          \"Rachel\", \"34\", \"Saleswoman\", \"1987\", 0\n        ],\n        [\n          \"Ross\", \"34\", \"Barber\", \"1987\", 0\n        ],\n        [\n          \"Chandler\", \"30\", \"Salesman\", \"1991\", 0\n        ]\n      ],\n      rows: [],\n      columns: [\n        'Name',\n        'Age',\n        'Profession',\n        'Year of birth',\n          \"skew\"\n      ],\n      sortIndex: null,\n      sortDirection: null\n    }\n  },\n  methods: {\n    sortRecords(index) {\n      if (this.sortIndex === index) {\n        switch (this.sortDirection) {\n          case null:\n            this.sortDirection = 'asc';\n            break;\n          case 'asc':\n            this.sortDirection = 'desc';\n            break;\n          case 'desc':\n            this.sortDirection = null;\n            break;\n        }\n      } else {\n        this.sortDirection = 'asc'\n      }\n\n      this.sortIndex = index;\n\n      if (!this.sortDirection) {\n        this.rows = performSearch(this.rawRows, this.term);\n        return;\n      }\n\n      this.rows = this.rows.sort(\n          (rowA, rowB) => {\n            if (this.sortDirection === 'desc') {\n              return rowB[index].localeCompare(rowA[index]);\n            }\n\n            return rowA[index].localeCompare(rowB[index]);\n          }\n      )\n    },\n    onSearch(e) {\n      this.term = e.target.value;\n      this.rows = performSearch(this.rawRows, this.term);\n    }\n  },\n  mounted() {\n    this.rows = [...this.rawRows];\n  }\n}\n</script>\n"]}]}